  "container": {
    "image": "192.168.0.246:5000/chameleon-chat-api:latest",
    "shell": "bash",
    "privileged": false,
    "network_model": "bridge",
    "web_ui": {
      "http": "8088",
      "path": "/"
    },
    "envs": [
      {
        "key": "TZ",
        "value": "Etc/UTC",
        "configurable": "yes",
        "description": "Timezone"
      }
    ],
    "ports": [
      {
        "container": "8080",
        "host": "8088",
        "type": "tcp",
        "allocation": "required",
        "configurable": "no",
        "description": "Http server"
      }
    ],
    "volumes": [],
    "devices": [],
    "constraints": {
      "min_memory": 1024,
      "min_storage": 1024
    },
    "restart_policy": "unless-stopped",
    "sysctls": [],
    "cap_add": [],
    "labels": []
  },
  "abilities": {
    "notification": false,
    "widgets": false,
    "authentication": false,
    "search": false,
    "upnp": false
  },
  "tips": {}
